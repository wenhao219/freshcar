{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "no-console": false,
    "no-var-requires": false,
    "forin": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case",
        "allow-snake-case"
      ]
    }
  },
  "rulesDirectory": []
}
